{
  "rules": {
      "no-console": "off",
      "eqeqeq": ["error", "always"],
      "semi": ["error", "always"]
  },
  "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "jest": true
  },
  "extends": [
      "eslint:recommended",
      "prettier"
  ],
  "globals": {
      "module": true,
      "require": true,
      "process": true,
      "connect": true
  },
  "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
  }
}